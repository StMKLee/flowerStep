
page{
	background-color: #ecf4c4;
}
.title {
	margin-left: 10rpx;
	margin-right: 10rpx;
	margin-top: 10rpx;
	padding: 10rpx;
	color: #FEC728;
	font-size: 60rpx;
	font-weight: 600;
	text-align: center;
}
.achievement-logo {
	width: 50rpx;
	height: 50rpx;
	position: absolute;
	top: 20rpx;
	left: 20rpx;
}
.achievement-item {
	position: relative;
	margin-left: 10rpx;
	margin-right: 10rpx;
	margin-top: 10rpx;
	padding: 10rpx;
	border: #000000 2rpx solid;
	border-radius: 5%;
	text-align: center;
	background-color: #E08047;
	color: #FEC728;
	font-size: 40rpx;
}
.container{
	margin-bottom: 50rpx;
}
.checked{
	background-color: #d9d2cd;
	color: #ffffff;
}
.tips{
	font-size: 25rpx;
	color: #FEC728;
}
.checked .tips{
	font-size: 25rpx;
	color: #FFFFFF;
}

