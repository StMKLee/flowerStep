}
.Logo{
	margin-top: 80rpx;
	height: 200rpx;
	width: 750rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.logoimg{
	height: 200rpx;
	width: 200rpx;
	border-radius: 20rpx;
	background-image: url(data:image/png;base64,);
